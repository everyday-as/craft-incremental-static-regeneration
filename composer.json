{
  "name": "everyday/craft-incremental-static-regeneration",
  "version": "1.0.0",
  "description": "A plugin that watches your entry changes and pings a URL of your choice",
  "type": "craft-plugin",
  "keywords": ["incremental", "static", "regeneration", "nextjs"],
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "Everyday AS",
      "homepage": "https://everyday.no"
    }
  ],
  "support": {
    "issues": "https://github.com/developer/repo/issues?state=open",
    "docs": "https://github.com/developer/repo/blob/master/README.md"
  },
  "autoload": {
    "psr-4": {
      "everyday\\IncrementalStaticRegeneration\\": "src/"
    }
  },
  "require": {
    "craftcms/cms": "^4.0"
  },
  "require-dev": {
    "vlucas/phpdotenv": "^3.0"
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "extra": {
    "name": "Incremental Static Regeneration",
    "handle": "incremental-static-regeneration",
    "class": "everyday\\IncrementalStaticRegeneration\\IncrementalStaticRegeneration"
  }
}